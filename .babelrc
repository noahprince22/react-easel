{
  "presets": ["es2015", "stage-0", "react"],
  "plugins": [
    "transform-decorators-legacy"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-runtime",
        ["react-transform", {
          "transforms": [{
            "transform" : "react-transform-hmr",
            "imports"   : ["react"],
            "locals"    : ["module"]
          }]
        }]
      ]
    }
  }
}
